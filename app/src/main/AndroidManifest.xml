<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gaby.amigablecasa">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Mi Casa Amigable"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AmigableCasa">
        <activity android:name=".basura_inorganica"></activity>
        <activity android:name=".basura_organica" />
        <activity android:name=".avance_basura" />
        <activity android:name=".idea_botes" />
        <activity android:name=".Consejo_lavado" />
        <activity android:name=".Consejo_lluvia" />
        <activity android:name=".Consejo_wc" />
        <activity android:name=".Consejo_cocina" />
        <activity android:name=".MenuConsejos" />
        <activity android:name=".Consejos" />
        <activity
            android:name=".MenuPrincipal"
            android:label="@string/title_activity_menu_principal"
            android:theme="@style/Theme.AmigableCasa.NoActionBar" />
        <activity android:name=".Formulario" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>